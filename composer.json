{
    "name": "olssonm/l5-humans",
    "description": "Simple humans.txt routes and views for Laravel 5.",
    "keywords": [
        "olssonm",
        "Laravel 5",
        "humans.txt"
    ],
    "homepage": "https://github.com/olssonm/l5-humans",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "orchestra/testbench-browser-kit": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\Humans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\Humans\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "Olssonm\\Humans\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
